    "react-dom": "^17.0.2",
    "react-hook-form": "^6.15.8",
    "react-select": "^4.3.1",
    "react-table": "^7.7.0",
    "react-toast-notifications": "^2.4.4",
    "rough-notation": "^0.5.1",
    "sentry-testkit": "^3.3.7",
    "superjson": "^1.7.5",
    "swr": "^0.5.7",
    "use-debounce": "^7.0.1",
    "uuid": "^8.3.2",
