{
  "name": "onvs",
  "version": "2.15.2",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "node scripts/server.js",
    "build": "next build",
    "start": "yarn migrate:latest && NODE_ENV=production node scripts/server.js",
    "lint": "eslint .",
    "test": "jest",
    "migrate:make": "node -r dotenv/config ./node_modules/knex/bin/cli migrate:make name_migration",
    "migrate:latest": "node -r dotenv/config ./node_modules/knex/bin/cli migrate:latest",
    "migrate:rollback": "node -r dotenv/config ./node_modules/knex/bin/cli migrate:rollback",
    "migrate:list": "node -r dotenv/config ./node_modules/knex/bin/cli migrate:list",
    "seed:make": "node -r dotenv/config ./node_modules/knex/bin/cli seed:make name_seed",
    "seed:run:dev": "node -r dotenv/config ./node_modules/knex/bin/cli seed:run --env development",
    "seal-secrets": "sre-secrets --to=./.temp-secrets",
    "k8s": "yarn --silent --cwd .k8s"
  },
  "dependencies": {
    "@headlessui/react": "^1.2.0",
    "@heroicons/react": "^1.0.0",
    "@hookform/resolvers": "^0.1.1",
    "@mdx-js/loader": "^1.6.22",
    "@next/mdx": "^10.1.3",
    "@prisma/client": "^2.23.0",
    "@sentry/browser": "^6.3.0",
    "@sentry/integrations": "^6.3.0",
    "@sentry/node": "^6.3.0",
    "@socialgouv/bootstrap.core": "^0.2.1",
    "@zeit/next-source-maps": "^0.0.3",
    "bcryptjs": "^2.4.3",
    "date-fns": "^2.21.1",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "knex": "^0.95.4",
    "little-state-machine": "^3.1.4",
    "lodash": "^4.17.21",
    "micro-cors": "^0.1.1",
    "next": "10.2.0",
    "next-compose-plugins": "^2.2.1",
    "next-iron-session": "^4.1.13",
    "nodemailer": "^6.5.0",
    "objects-to-csv": "^1.3.6",
    "pg": "^8.6.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-hook-form": "^6.15.6",
    "react-select": "^4.3.0",
    "react-toast-notifications": "^2.4.3",
    "rough-notation": "^0.5.1",
    "sentry-testkit": "^3.3.2",
    "superjson": "^1.7.4",
    "swr": "^0.5.5",
    "uuid": "^8.3.2",
    "yup": "^0.32.9",
    "zod": "^3.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.1.1",
    "@commitlint/config-conventional": "^12.1.1",
    "@hookform/devtools": "^2.2.1",
    "@socialgouv/eslint-config-react": "^1.70.0",
    "@socialgouv/eslint-config-recommended": "^1.70.0",
    "@socialgouv/eslint-config-typescript": "^1.70.0",
    "@socialgouv/sre-secrets": "^1.4.0",
    "@tailwindcss/forms": "^0.3.2",
    "@testing-library/jest-dom": "^5.11.10",
    "@testing-library/react": "^11.2.6",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.23",
    "@types/node": "^15.3.0",
    "@types/react": "^17.0.6",
    "@typescript-eslint/eslint-plugin": "^4.24.0",
    "@typescript-eslint/parser": "^4.24.0",
    "autoprefixer": "^10.2.5",
    "babel-plugin-superjson-next": "^0.3.0",
    "eslint": "^7.26.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.2.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-jest": "^24.3.6",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "husky": "^4.3.8",
    "jest": "^26.6.3",
    "postcss": "8.2.15",
    "prettier": "^2.3.0",
    "prettier-eslint": "^12.0.0",
    "prisma": "^2.23.0",
    "react-select-event": "^5.3.0",
    "tailwindcss": "2.1.2",
    "typescript": "^4.2.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
