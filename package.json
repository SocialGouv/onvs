{
  "name": "onvs",
  "version": "2.6.2",
  "private": true,
  "license": "MIT",
  "scripts": {
    "dev": "node scripts/server.js",
    "build": "next build",
    "start": "yarn migrate:latest && NODE_ENV=production node scripts/server.js",
    "lint": "eslint src/**/*.js",
    "test": "jest",
    "migrate:make": "node -r dotenv/config ./node_modules/knex/bin/cli migrate:make name_migration",
    "migrate:latest": "node -r dotenv/config ./node_modules/knex/bin/cli migrate:latest",
    "migrate:rollback": "node -r dotenv/config ./node_modules/knex/bin/cli migrate:rollback",
    "migrate:list": "node -r dotenv/config ./node_modules/knex/bin/cli migrate:list",
    "seed:make": "node -r dotenv/config ./node_modules/knex/bin/cli seed:make name_seed",
    "seed:run:dev": "node -r dotenv/config ./node_modules/knex/bin/cli seed:run --env development",
    "seed:run:staging": "node -r dotenv/config ./node_modules/knex/bin/cli seed:run --env staging",
    "seal-secrets": "sre-secrets",
    "k8s": "yarn --silent --cwd .k8s"
  },
  "dependencies": {
    "@hookform/resolvers": "^0.1.1",
    "@sentry/browser": "^6.2.2",
    "@sentry/integrations": "^6.2.2",
    "@sentry/node": "^6.2.2",
    "@socialgouv/bootstrap.core": "^0.2.1",
    "@tailwindui/react": "^0.1.1",
    "@zeit/next-source-maps": "^0.0.3",
    "date-fns": "^2.15.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "isomorphic-unfetch": "^3.0.0",
    "knex": "^0.95.1",
    "little-state-machine": "^3.1.4",
    "lodash": "^4.17.21",
    "micro-cors": "^0.1.1",
    "next": "^10.0.8",
    "next-compose-plugins": "^2.2.1",
    "next-transpile-modules": "^6.3.0",
    "nodemailer": "^6.4.18",
    "objects-to-csv": "^1.3.6",
    "pg": "^8.3.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-hook-form": "^6.3.3",
    "react-select": "^4.2.1",
    "react-toast-notifications": "^2.4.3",
    "rough-notation": "^0.5.1",
    "sentry-testkit": "^3.2.4",
    "swr": "^0.5.2",
    "uuid": "^8.3.2",
    "yup": "^0.32.9"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@hookform/devtools": "^2.0.5",
    "@socialgouv/eslint-config-react": "^1.33.0",
    "@socialgouv/eslint-config-recommended": "^1.33.0",
    "@socialgouv/sre-secrets": "^1.1.7",
    "@tailwindcss/custom-forms": "^0.2.1",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.0.4",
    "@testing-library/user-event": "^12.1.10",
    "eslint": "^7.6.0",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^3.1.4",
    "husky": "^4.3.8",
    "jest-cli": "^26.2.2",
    "little-state-machine-devtools": "^1.0.0",
    "postcss-preset-env": "^6.7.0",
    "prettier": "^2.0.5",
    "react-select-event": "^5.0.0",
    "tailwindcss": "^1.6.3"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  }
}
