    "eslint-plugin-react-hooks": "^4.2.0",
    "faker": "^5.5.3",
    "jest": "^27.0.6",
    "postcss": "^8.3.11",
    "prettier": "^2.3.2",
    "prettier-eslint": "^12.0.0",
    "prisma": "^2.25.0",
    "react-select-event": "^5.3.0",
    "tailwindcss": "^2.2.19",
    "typescript": "^4.3.5"
  },
  "commitlint": {
